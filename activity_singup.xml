<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SingupActivity">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="156dp"
        android:layout_height="140dp"
        android:background="@drawable/magix"
        android:text="Hello World!"
        android:layout_marginTop="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.045" /><![CDATA[







    EditText
        android:id="@+id/editText8"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="340dp"
        android:ems="10"
        android:hint="Date-of-Birth"
        android:inputType="textPersonName"
        android:textColor="#1f3566"
        android:textColorHint="#295788"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/editText5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/editText5" />

    EditText
        android:id="@+id/editText9"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Address"
        android:inputType="textPersonName"
        android:textColorHint="#295788"
        android:textColor="#1f3566"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/editText8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/editText8"
        app:layout_constraintTop_toBottomOf="@+id/editText8"
        app:layout_constraintVertical_bias="0.0" />



















    ]]>

    <EditText
        android:id="@+id/fNameTV"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:drawablePadding="5dp"
        android:drawableLeft="@drawable/ic_person_black_24dp"
        android:hint="First Name"
        android:inputType="textPersonName"
        android:textColor="#1f3566"
        android:textColorHint="#295788"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.426"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.074" />

    <EditText
        android:id="@+id/lName"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:drawablePadding="5dp"
        android:drawableLeft="@drawable/ic_person_black_24dp"
        android:hint="Last Name"
        android:inputType="textPersonName"
        android:textColor="#1f3566"
        android:textColorHint="#295788"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/fNameTV"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/fNameTV"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.171" />

    <EditText
        android:id="@+id/mailTX"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:drawablePadding="5dp"
        android:drawableLeft="@drawable/ic_email_black_24dp"
        android:hint="Email"
        android:inputType="textEmailAddress"
        android:textColor="#1f3566"
        android:textColorHint="#295788"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/lName"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/lName"
        app:layout_constraintTop_toBottomOf="@+id/lName"
        app:layout_constraintVertical_bias="0.028" />

    <EditText
        android:id="@+id/passwordTX"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="288dp"
        android:ems="10"
        android:drawablePadding="5dp"
        android:drawableLeft="@drawable/ic_vpn_key_black_24dp"
        android:hint="Password"
        android:inputType="textPassword"
        android:textColorHint="#295788"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/mailTX"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/mailTX" />

    <EditText
        android:id="@+id/retypePasswrd"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:hint="Confirm Password"
        android:inputType="textPassword"
        android:drawablePadding="5dp"
        android:drawableLeft="@drawable/ic_vpn_key_black_24dp"
        android:textColor="#1f3566"
        android:textColorHint="#295788"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/passwordTX"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/passwordTX"
        app:layout_constraintTop_toBottomOf="@+id/passwordTX"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/createAcountBtn"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:text="Create Account"
        android:textColor="#FFFFFF"
        android:background="@drawable/login_btn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/retypePasswrd"
        app:layout_constraintStart_toStartOf="@+id/retypePasswrd"
        app:layout_constraintTop_toBottomOf="@+id/retypePasswrd" />


</androidx.constraintlayout.widget.ConstraintLayout>